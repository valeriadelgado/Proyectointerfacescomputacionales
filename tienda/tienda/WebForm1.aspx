 <script type="text/javascript">


    $(window).load(function () {
        $(document).ready(function(){
            collage();
            $('.Collage').collageCaption();
			$('#tab_datos').css("visibility","visible");
			
			//operaciones por cubo
			$("#tipos_cubos").change(function() { 
             var clave=$("#tipos_cubos").val();
                  location.href=BASE_URL+'cubo/id/'+clave;
             });
			//si se ha un cambio de categorias
			$("#categorias").change(function() { 
             var clave=$("#categorias").val();
                 //hacer una llamada ajax y colocarlo en tabdatos
			$('#tab_datos').css("visibility","hidden");

			var dataString ='clave='+clave;
              $.ajax({
                type: "POST",
                url:BASE_URL+"cubo/categorias_proveedores/",
                data: dataString,
                success: function(data){
                 $("#tab_datos").html(data);
				   collage();
                  $('.Collage').collageCaption();
				 $('#tab_datos').css("visibility","visible");

                }
            });	
			
             });
			
        });
		
		//al dar click en restaurant
		   $('#tab_datos').delegate( '.accion_user', 'click', function(){
           var clave=$(this).data('clave');
		   
		   //tirr un aconx ajax
		    app.clase.cargardor.activo('modal_informacion');
             var dataString ='cv_restaurant='+clave;
              $.ajax({
                type: "POST",
                url:BASE_URL+"cubo/info_1/",
                data: dataString,
                success: function(data){
                 $("#modal_informacion").html(data);
                }
            });		   
         }); 
		
    });



    function collage() {
        $('.Collage').removeWhitespace().collagePlus(
            {
                'fadeSpeed'     : 8000,
                'targetHeight'  : 150
            }
        );
    };


    var resizeTimer = null;
    $(window).bind('resize', function() {
        $('.Collage .Image_Wrapper').css("opacity", 0);
        if (resizeTimer) clearTimeout(resizeTimer);
        resizeTimer = setTimeout(collage, 2000);
    });
	
	
             $("#comprar").click(function() { 
        	    $('#nueva-compra-modal').modal('show');
             });
			
    </script>
	
